namespace dictionary
{
	class KeyvaluePair
	{
		func KeyvaluePair(key, value)
		{
			this.key = key
			this.value = value
		}
	}

	class Dictionary
	{
		func Dictionary
		{
			this.data = list::List(null)
		}

		func add(key, value)
		{
			this.data.add(KeyvaluePair(key, value))
		}

		func get(key)
		{
			for(var _ in this.data)
			{
				if(_.key == key)
				{
					return _.value
				}
			}
			interrupt exception::KeyNotFoundException(key)
		}
	}
}